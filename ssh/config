Host bitbucket.org
    HostName bitbucket.org
    User git
    IdentityFile ~/.ssh/bitBkt
    ForwardX11 no

Host github.com
    AddKeysToAgent yes
    UseKeychain yes
    IdentityFile ~/.ssh/github_key

Host pknas
    HostName 10.0.0.154
    User bxchen
    ForwardAgent yes
    IdentityFile ~/.ssh/homelab
